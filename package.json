{
  "name": "aisdk-mcp-bridge",
  "version": "0.1.0",
  "description": "Bridge between Model Context Protocol (MCP) servers and AI SDK tools",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test:mcp": "ts-node examples/test-mcp.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "ai",
    "mcp",
    "ai-sdk",
    "bridge",
    "tools",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/aisdk-mcp-bridge.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/aisdk-mcp-bridge/issues"
  },
  "homepage": "https://github.com/yourusername/aisdk-mcp-bridge#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.4.1",
    "ai": "latest",
    "axios": "^1.6.7",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "ai": "^4.1.10"
  },
  "devDependencies": {
    "@ai-sdk/google": "^1.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
